    "caption": "DB Notebook",
    "content": "\\about\nCREATE DATABASE Student_Manage\nUSE Student_Manage\n-- 创建学院表\nCREATE TABLE College (\n    CollegeID INT AUTO_INCREMENT PRIMARY KEY,\n    CollegeName VARCHAR(255) NOT NULL\n);\n\n-- 创建专业表\nCREATE TABLE Major (\n    MajorID INT AUTO_INCREMENT PRIMARY KEY,\n    MajorName VARCHAR(255) NOT NULL,\n    CollegeID INT,\n    FOREIGN KEY (CollegeID) REFERENCES College(CollegeID)\n        ON DELETE SET NULL ON UPDATE CASCADE\n);\n\n\n-- 创建学生表\nCREATE TABLE Student (\n    StudentID VARCHAR(255) PRIMARY KEY,\n    Name VARCHAR(255) NOT NULL,\n    Gender VARCHAR(10),\n    DateOfBirth DATE,\n    ContactInfo VARCHAR(255),\n    EnrollmentYear INT,\n    MajorID INT,\n    FOREIGN KEY (MajorID) REFERENCES Major(MajorID)\n        ON DELETE SET NULL ON UPDATE CASCADE\n);\n\n-- 创建课程表\nCREATE TABLE Course (\n    CourseID INT AUTO_INCREMENT PRIMARY KEY,\n    CourseName VARCHAR(255) NOT NULL,\n    Credits INT,\n    OfferingCollegeID INT,\n    FOREIGN KEY (OfferingCollegeID) REFERENCES College(CollegeID)\n        ON DELETE SET NULL ON UPDATE CASCADE\n);\n\n-- 创建专业变更表\nCREATE TABLE MajorChange (\n    ChangeID INT AUTO_INCREMENT PRIMARY KEY,\n    StudentID VARCHAR(255),\n    ChangeDate DATE,\n    FOREIGN KEY (StudentID) REFERENCES Student(StudentID)\n        ON DELETE CASCADE ON UPDATE CASCADE\n);\n-- 创建奖惩记录表\nCREATE TABLE AwardPunishment (\n    APID INT AUTO_INCREMENT PRIMARY KEY,\n    StudentID VARCHAR(255),\n    Type VARCHAR(255),\n    Description TEXT,\n    Date DATE,\n    FOREIGN KEY (StudentID) REFERENCES Student(StudentID)\n        ON DELETE CASCADE ON UPDATE CASCADE\n);\n\n-- 创建选课表\nCREATE TABLE Enrollment (\n    EnrollmentID INT AUTO_INCREMENT PRIMARY KEY,\n    StudentID VARCHAR(255),\n    CourseID INT,\n    Semester VARCHAR(255),\n    Grade VARCHAR(10),\n    FOREIGN KEY (StudentID) REFERENCES Student(StudentID)\n        ON DELETE CASCADE ON UPDATE CASCADE,\n    FOREIGN KEY (CourseID) REFERENCES Course(CourseID)\n        ON DELETE CASCADE ON UPDATE CASCADE,\n    UNIQUE (StudentID, CourseID)  -- 添加唯一约束\n);\n\nCREATE TABLE Account (\n    UserID INT AUTO_INCREMENT PRIMARY KEY,\n    Username VARCHAR(255) NOT NULL UNIQUE,\n    Password VARCHAR(255) NOT NULL,\n    Role ENUM('student', 'admin') NOT NULL,\n    StudentID VARCHAR(255) NULL,\n    FOREIGN KEY (StudentID) REFERENCES Student(StudentID)\n);\n\nINSERT INTO account (username, password, role) VALUES ('root', '99adc231b045331e514a516b4b7680f588e3823213abe901738bc3ad67b2f6fcb3c64efb93d18002588d3ccc1a49efbae1ce20cb43df36b38651f11fa75678e8', 'admin');\nSELECT * FROM account\nSELECT password, role FROM account WHERE username = 'root'\nDROP TABLE student\nALTER TABLE Student\nADD MajorID INT;\n\nALTER TABLE Student\nADD CONSTRAINT fk_student_major\nFOREIGN KEY (MajorID) REFERENCES Major(MajorID)\nON DELETE SET NULL\nON UPDATE CASCADE;\n\nALTER TABLE Student MODIFY MajorID INT NOT NULL;\nSELECT CONSTRAINT_NAME \nFROM INFORMATION_SCHEMA.KEY_COLUMN_USAGE \nWHERE TABLE_NAME = 'Account' \n      AND COLUMN_NAME = 'StudentID' \n\nALTER TABLE Account DROP FOREIGN KEY account_ibfk_1;\n\nALTER TABLE Account\nADD CONSTRAINT account_ibfk_1\nFOREIGN KEY (StudentID) REFERENCES Student(StudentID)\nON DELETE CASCADE;\n\nALTER TABLE Student DROP FOREIGN KEY fk_student_major;\n\nALTER TABLE Student\nADD CONSTRAINT fk_student_major\nFOREIGN KEY (MajorID) REFERENCES Major(MajorID)\nON DELETE CASCADE;\n\n-- 事务用来变更专业\nDELIMITER //\n\nCREATE PROCEDURE ChangeStudentMajor(\n    IN student_id VARCHAR(255),\n    IN new_major_id INT,\n    IN change_date DATE\n)\nBEGIN\n    DECLARE old_major_id INT;\n    DECLARE exit handler for sqlexception\n    BEGIN\n        ROLLBACK;\n        RESIGNAL;\n    END;\n\n    START TRANSACTION;\n\n    SELECT MajorID INTO old_major_id FROM Student WHERE StudentID = student_id;\n\n    UPDATE Student SET MajorID = new_major_id WHERE StudentID = student_id;\n\n    INSERT INTO MajorChange (StudentID, OldMajorID, NewMajorID, ChangeDate)\n    VALUES (student_id, old_major_id, new_major_id, change_date);\n\n    COMMIT;\nEND;\n\n//\nDELIMITER ;\n\n\nALTER TABLE MajorChange\nADD COLUMN OldMajorID INT,\nADD COLUMN NewMajorID INT,\nADD CONSTRAINT fk_old_major_id FOREIGN KEY (OldMajorID) REFERENCES Major(MajorID) ON DELETE SET NULL ON UPDATE CASCADE,\nADD CONSTRAINT fk_new_major_id FOREIGN KEY (NewMajorID) REFERENCES Major(MajorID) ON DELETE SET NULL ON UPDATE CASCADE;\n\nCALL ChangeStudentMajor('PB21114514', 2, '2023-01-01');\nSELECT CONSTRAINT_NAME\nFROM INFORMATION_SCHEMA.TABLE_CONSTRAINTS\nWHERE TABLE_SCHEMA = 'student_manage'\n  AND TABLE_NAME = 'Enrollment'\n  AND CONSTRAINT_TYPE = 'UNIQUE';\n\nSELECT DISTINCT CONSTRAINT_NAME\nFROM INFORMATION_SCHEMA.TABLE_CONSTRAINTS\nWHERE TABLE_SCHEMA = 'student_manage' -- 替换成你的数据库名\n  AND TABLE_NAME = 'Enrollment'\n  AND CONSTRAINT_TYPE = 'UNIQUE';\n\n\nALTER TABLE Enrollment\nDROP INDEX `Course`\nSHOW INDEX FROM Enrollment;\n\nALTER TABLE Enrollment\nDROP INDEX `StudentID`;\n\nSHOW CREATE TABLE Enrollment\nSELECT CONSTRAINT_NAME\nFROM INFORMATION_SCHEMA.KEY_COLUMN_USAGE\nWHERE TABLE_SCHEMA = 'student_manage' -- 替换成你的数据库名\n  AND TABLE_NAME = 'Enrollment'\n  AND REFERENCED_TABLE_NAME IS NOT NULL;\n\nSELECT CONSTRAINT_NAME, COLUMN_NAME, REFERENCED_TABLE_NAME, REFERENCED_COLUMN_NAME\nFROM INFORMATION_SCHEMA.KEY_COLUMN_USAGE\nWHERE TABLE_SCHEMA = 'student_manage' -- 替换成你的数据库名\n  AND TABLE_NAME = 'Enrollment'\n  AND REFERENCED_TABLE_NAME IS NOT NULL;\n\nALTER TABLE Enrollment DROP FOREIGN KEY `enrollment_ibfk_1`\nALTER TABLE Enrollment DROP INDEX `StudentID`;\n\nALTER TABLE Enrollment\nADD CONSTRAINT `fk_StudentID`\nFOREIGN KEY (`StudentID`) REFERENCES Student(`StudentID`) ON DELETE CASCADE ON UPDATE CASCADE;\n\nDELIMITER //\n\nCREATE PROCEDURE RegisterCourse(\n    IN p_student_id VARCHAR(255),\n    IN p_course_id INT,\n    IN p_semester VARCHAR(255)\n)\nBEGIN\n    -- 检查学生是否已经在同一学期选了同一门课程\n    DECLARE v_exists INT;\n    SELECT COUNT(*) INTO v_exists FROM Enrollment\n    WHERE StudentID = p_student_id AND CourseID = p_course_id AND Semester = p_semester;\n\n    IF v_exists = 0 THEN\n        -- 如果没有重复记录，则插入新的选课记录，并将成绩初始化为 NULL\n        INSERT INTO Enrollment (StudentID, CourseID, Semester, Grade)\n        VALUES (p_student_id, p_course_id, p_semester, NULL);\n    ELSE\n        -- 如果已存在，抛出错误信息\n        SIGNAL SQLSTATE '45000' SET MESSAGE_TEXT = 'This course has already been registered by the student for the given semester.';\n    END IF;\nEND;\n\n//\nDELIMITER ;\n\n\nDELIMITER //\n\nCREATE FUNCTION CalculateWeightedAverageGrade(student_id VARCHAR(255)) \nRETURNS DECIMAL(5,2)\nREADS SQL DATA \nBEGIN\n    DECLARE total_weighted_scores DECIMAL(10,2);\n    DECLARE total_credits DECIMAL(10,2);\n    DECLARE weighted_average DECIMAL(5,2);\n\n    SET total_weighted_scores = 0;\n    SET total_credits = 0;\n\n    SELECT \n        SUM(CAST(Grade AS DECIMAL(5,2)) * Credits), \n        SUM(Credits)\n    INTO \n        total_weighted_scores, \n        total_credits\n    FROM Enrollment\n    JOIN Course ON Enrollment.CourseID = Course.CourseID\n    WHERE Enrollment.StudentID = student_id AND Enrollment.Grade IS NOT NULL AND Enrollment.Grade != '';\n\n    IF total_credits > 0 THEN\n        SET weighted_average = total_weighted_scores / total_credits;\n    ELSE\n        SET weighted_average = 0;\n    END IF;\n\n    RETURN weighted_average;\nEND;\n\n//\nDELIMITER ;\n\n\n\n\nALTER TABLE Major\nADD COLUMN StudentCount INT DEFAULT 0;\n\nUPDATE Major\nSET StudentCount = (\n    SELECT COUNT(*)\n    FROM Student\n    WHERE Student.MajorID = Major.MajorID\n);\n\nDELIMITER //\n\nCREATE TRIGGER UpdateMajorCount\nAFTER UPDATE ON Student\nFOR EACH ROW\nBEGIN\n    IF OLD.MajorID <> NEW.MajorID THEN\n        UPDATE Major\n        SET StudentCount = StudentCount - 1\n        WHERE MajorID = OLD.MajorID;\n\n        UPDATE Major\n        SET StudentCount = StudentCount + 1\n        WHERE MajorID = NEW.MajorID;\n    END IF;\nEND;\n\n//\nDELIMITER ;\n\nALTER TABLE Student\nADD COLUMN Photo BLOB;\n\nALTER TABLE Student\nMODIFY COLUMN Photo LONGBLOB;\n\nDELIMITER //\n\nCREATE TRIGGER DecrementMajorCount\nAFTER DELETE ON Student\nFOR EACH ROW\nBEGIN\n    UPDATE Major\n    SET StudentCount = StudentCount - 1\n    WHERE MajorID = OLD.MajorID;\nEND;\n\n//\nDELIMITER ;\n\nDELIMITER //\n\nCREATE TRIGGER IncrementMajorCount\nAFTER INSERT ON Student\nFOR EACH ROW\nBEGIN\n    UPDATE Major\n    SET StudentCount = StudentCount + 1\n    WHERE MajorID = NEW.MajorID;\nEND;\n\n//\nDELIMITER ;\n\n\n",
